pipeline {
    agent any
    
    stages() {
        stage('git clone') {
            steps() {
                git 'https:ithub.com/jump99500/jenkins-pipeline.git'
            }
        }
        
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        
        stage('execute sh') {
            steps {
                sh "chmod 774 ./project.sh"
                sh "./project.sh"
            }
        }        
    }


